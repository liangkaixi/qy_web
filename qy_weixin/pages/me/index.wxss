.me-page {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 40rpx 0;
}
.me-avatar-section {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-bottom: 40rpx;
}
.avatar {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  margin-bottom: 20rpx;
}
.nickname {
  font-size: 32rpx;
  color: #333;
}
button {
  width: 80%;
  margin-top: 40rpx;
}
